{

    "type": "origins:multiple",

    "toggle": {
        "type": "origins:toggle",
        "active_by_default": false,
        "key": {
            "key": "key.origins.secondary_active"
        }
    },
    "invisibility": {
        "type": "origins:invisibility",
        "render_armor": false,
        "condition": {
            "type": "origins:power_active",
            "power": "*:*_toggle"
        }
    },
    "water_trail":{
        "type": "origins:action_over_time",
        "entity_action": {
            "type": "origins:block_action_at",
            "block_action": {
                "type": "origins:if_else",
                "condition": {
                    "type": "origins:fluid",
                    "fluid_condition": {
                        "type": "origins:still"
                    }
                },
                "if_action": {
                    "type": "origins:nothing"
                },
                "else_action": {
                    "type": "origins:and",
                    "actions": [
                        {
                            "type": "origins:add_block",
                            "block": "minecraft:water"
                        },
                        {    
                            "type": "origins:delay",
                            "ticks": 5,
                            "action": {
                                "type": "origins:add_block",
                                "block": "minecraft:air"
                            }
                        }
                    ]
                }
            }
        },
        "interval": 1,
        "condition": {
            "type": "origins:and",
            "conditions": [
                {
                    "type": "origins:power_active",
                    "power": "*:*_toggle"
                },
                {      
                    "type": "origins:dimension",
                    "dimension": "minecraft:the_nether",
                    "inverted": true
                }
            ]
        }
    },
    "one_block_tall":{
        "type": "origins:active_self",
        "entity_action":{
            "type": "origins:and",
            "actions": [
            {
                "type": "origins:execute_command",
                "command": "scale set pehkui:height 0.5 @s"
            },
            {
                "type": "origins:execute_command",
                "command": "scale set pehkui:width 0.5 @s"
            },
            {
                "type": "origins:execute_command",
                "command": "scale persist set true @s"
            }
            ]
        },
        "key": {
            "key": "key.origins.secondary_active"
        },
        "condition": {
            "type": "origins:power_active",
            "power": "*:*_toggle",
            "inverted": true
        }
    },
    "normal_height":{
        "type": "origins:active_self",
        "entity_action":{
            "type": "origins:and",
            "actions": [
            {
                "type": "origins:execute_command",
                "command": "scale set pehkui:height 1 @s"
            },
            {
                "type": "origins:execute_command",
                "command": "scale set pehkui:width 1 @s"
            },
            {
                "type": "origins:execute_command",
                "command": "scale persist set true @s"
            }
            ]
        },
        "key": {
            "key": "key.origins.secondary_active"
        },
        "condition": {
            "type": "origins:power_active",
            "power": "*:*_toggle"
        }
    },
    "become_water":{
        "type": "origins:ignore_water",
        "condition": {
            "type": "origins:power_active",
            "power": "*:*_toggle"
        }
    },
    "breathe_underwater":{
        "type": "origins:action_over_time",
        "entity_action": {
            "type": "origins:apply_effect",
            "effects": [
                {
                    "effect": "minecraft:water_breathing",
                    "duration": 20,
                    "is_ambient": false,
                    "show_particles": false,
                    "show_icon": true
                }
            ]
        },
        "interval": 10,
        "condition": {
            "type": "origins:and",
            "conditions": [
                {
                    "type": "origins:power_active",
                    "power": "*:*_toggle"
                },
                {      
                    "type": "origins:submerged_in",
                    "fluid": "minecraft:water"
                }
            ]
        }
    },
    "creative_fly_underwater":{
        "type": "origins:creative_flight",
        "condition": {
            "type": "origins:and",
            "conditions": [
                {
                    "type": "origins:power_active",
                    "power": "*:*_toggle"
                },
                {      
                    "type": "origins:submerged_in",
                    "fluid": "minecraft:water"
                }
            ]
        }
    },
    "clear_vision_underwater":{
        "type": "origins:toggle_night_vision",
        "strength": 1.0,
        "active_by_default": false,
        "condition": {
            "type": "origins:and",
            "conditions": [
                {
                    "type": "origins:power_active",
                    "power": "*:*_toggle"
                },
                {      
                    "type": "origins:submerged_in",
                    "fluid": "minecraft:water"
                }
            ]
        }
    },
    "instant_death_nether":{
        "type": "origins:action_over_time",
        "entity_action": {
            "type": "origins:damage",
            "amount": 1000,
            "source": {
                "name": "dryout",
                "bypasses_armor": true
            }
        },
        "interval": 1,
        "condition": {
            "type": "origins:and",
            "conditions": [
                {
                    "type": "origins:power_active",
                    "power": "*:*_toggle"
                },
                {      
                    "type": "origins:dimension",
                    "dimension": "minecraft:the_nether"
                }
            ]
        }
    },
    "name": "Meld into Water",
    "description": "You can turn yourself into water. Anything and anywhere that can instantly dry up water will dry you up in an instant, killing you."

}
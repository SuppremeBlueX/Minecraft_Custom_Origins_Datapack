{

    "type": "origins:multiple",

    "toggle": {
        "type": "origins:toggle",
        "active_by_default": false,
        "retain_state": true,
        "key": {
            "key": "key.origins.secondary_active"
        },
        "condition": {
            "type": "origins:fall_flying"
        }  
    },

    "phase_flight": {
        "type": "origins:phasing",
        "blacklist": true,
        "render_type": "none",
        "block_condition": {
            "type": "origins:in_tag",
            "tag": "origins:unphasable"
        },
        "condition": {
            "type": "origins:power_active",
            "power": "*:*_toggle"
        }
    }

}
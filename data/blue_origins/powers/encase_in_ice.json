{

    "type": "origins:multiple",

    "toggle": {
        "type": "origins:toggle",
        "active_by_default": false,
        "condition": {
            "type": "origins:resource",
            "resource": "blue_origins:ice_charge",
            "comparison": "==",
            "compare_to": 100
        }
    },

    "invulnerability": {
        "type": "origins:invulnerability",
        "condition": {
            "type": "origins:power_active",
            "power": "*:*_toggle"
        }
    },

    "self_freeze": {
        "type": "origins:action_over_time",
        "entity_action": {
            "type": "origins:delay",
            "ticks": 400,
            "action":{
                "type": "origins:toggle",
                "power": "*:*_toggle"
            }
        },
        "interval": 20,
        "condition": {
            "type": "origins:power_active",
            "power": "*:*_toggle"
        }
    }

}